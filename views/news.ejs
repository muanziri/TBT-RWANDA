   }
   
   window.addEventListener('scroll',()=>{
     
      if(window.scrollY==0){
       document.querySelector('#appearOnZero').style.display='flex'
      }else{
        document.querySelector('#appearOnZero').style.display='none'
      }
   })

    let options = {
      root: null,
      rootmargin: '0px',
      threshold: 0.25
    }
    let callback = (entreis, observer) => {
      entreis.forEach(entry => {
        if (entry.IsIntersecting && entry.target.className === 'newsTabsImages') {
          let imageUrl = entry.target.getAttribute('data-img');
          if (imageUrl) {
            entry.target.src = imageUrl;
            observer.unobserve(entry.target);
          }
        }

      });

    }
    let observer = new IntersectionObserver(callback, options);
