  <script>


    let GosssipNews = document.querySelector('#GosssipNews');
    let GosssipNewsButton = document.querySelector('#buttonChanger1');
    let latestShowbizz = document.querySelector('#latestShowbizz');
    let latestShowbizzButton = document.querySelector('#buttonChanger');
    let showbizzTrendingNews = document.querySelector('#showbizzTrendingNews');
    let showbizzTrendingNewsButton = document.querySelector('#buttonChanger2');
    function latestShowbizzShow() {
      latestShowbizz.style.display = 'flex'
      GosssipNewsButton.style.backgroundColor = 'rgb(94, 39, 0)'
      showbizzTrendingNewsButton.style.backgroundColor = 'rgb(94, 39, 0)'
      latestShowbizzButton.style.backgroundColor = 'black'
      GosssipNews.style.display = 'none'
      showbizzTrendingNews.style.display = 'none'
    }
    function showbizzTrendingNewsShow() {
      latestShowbizz.style.display = 'none'
      GosssipNewsButton.style.backgroundColor = 'rgb(94, 39, 0)'
      showbizzTrendingNewsButton.style.backgroundColor = 'black'
      latestShowbizzButton.style.backgroundColor = 'rgb(94, 39, 0)'
      GosssipNews.style.display = 'none'
      showbizzTrendingNews.style.display = 'flex'
    }
    function GosssipNewsShow() {
      latestShowbizz.style.display = 'none'
      GosssipNews.style.display = 'flex'
      GosssipNewsButton.style.backgroundColor = 'black'
      showbizzTrendingNewsButton.style.backgroundColor = 'rgb(94, 39, 0)'
      latestShowbizzButton.style.backgroundColor = 'rgb(94, 39, 0)'
      showbizzTrendingNews.style.display = 'none'
    }

    window.addEventListener('scroll', () => {

      if (window.scrollY == 0) {
        document.querySelector('#appearOnZero').style.display = 'flex'
      } else {
        document.querySelector('#appearOnZero').style.display = 'none'
      }
    })

    let options = {
      root: null,
      rootmargin: '0px',
      threshold: 0.25
    }
    let callback = (entreis, observer) => {
      entreis.forEach(entry => {
        if (entry.IsIntersecting && entry.target.className === 'newsTabsImages') {
          let imageUrl = entry.target.getAttribute('data-img');
          if (imageUrl) {
            entry.target.src = imageUrl;
            observer.unobserve(entry.target);
          }
        }

      });

    }
    let observer = new IntersectionObserver(callback, options);
  </script>
